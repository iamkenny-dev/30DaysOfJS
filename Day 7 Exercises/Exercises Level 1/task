function findMax(a, b, c) {
var num1 = a;
var num2 = b;
var num3 = c;
        
	        if (num1 > num2 && num1 > num3) {
		            return("num1: " + num1 + " is greater");
			                
					        } else if (num2 > num1 && num2 > num3) {
						            return("num2: " + num2 + " is greater");
							            }
								            else {
									                return num3
											        }
												}       
												console.log(findMax(9, 2, 6));
